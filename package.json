{
  "name": "wefox-challenge",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.3.0",
    "@testing-library/user-event": "13.5.0",
    "@types/node": "16.11.48",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@types/styled-components": "5.1.26",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-markdown": "^8.0.5",
    "react-router-dom": "6.8.1",
    "react-scripts": "5.0.1",
    "styled-components": "5.3.6",
    "typescript": "4.9.5",
    "web-vitals": "3.1.1"
  },
  "scripts": {
    "analyze": "react-scripts build && source-map-explorer 'build/static/js/*.js'",
    "tslint": "tslint -c tslint.json -p tsconfig.json && tslint -c tslint.test.json -p tsconfig.test.json",
    "tslint-fix": "tslint -c tslint.json -p tsconfig.json --fix && tslint -c tslint.test.json -p tsconfig.test.json --fix",
    "prettier": "prettier --write 'src/**/*.{ts,tsx,js,jsx,scss,css,json}'",
    "start": "react-scripts start",
    "build": "react-scripts build && compress-cra",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "prepare": "husky install"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/jest": "27.5.2",
    "compress-create-react-app": "1.2.1",
    "husky": "8.0.3",
    "lint-staged": "13.0.3",
    "prettier": "2.8.4",
    "source-map-explorer": "2.5.3",
    "tslint": "6.1.3",
    "tslint-config-prettier": "1.18.0",
    "tslint-react": "5.0.0"
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!react-markdown)/"
    ]
  }
}